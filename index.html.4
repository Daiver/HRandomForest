	<script type="text/javascript">
		var FOTO_IS_IFOLDER_USER_AUTHORIZED = false;
		var FOTO_IFOLDER_USER_EMAIL = '';

		function get_file_name_from_path()
		{
			var file = $(this).val();
			reUnix = /.*[\/\\](.*)$/;
			file = file.replace(reUnix, "$1");
			$('#foto-name').text(file);
		}

		function ifolder_foto_upload_onsubmit()
		{
			//	functions used here are defined in FotoHosting in page_short_upload_ifolder_html.tt file
			if(CheckFileExt($('#uploadfotofield').val(), VALID_FOTO_ARCHIVES)) {
				if(FOTO_IS_IFOLDER_USER_AUTHORIZED && FOTO_IFOLDER_USER_EMAIL) {
					$('#anonym_email').val(FOTO_IFOLDER_USER_EMAIL);
				}
				else {
					var anonym_email = prompt("Для загрузки архива Вам необходимо указать email,\nна который придет оповещение о том, что архив распакован,\nа также ссылка на загруженные фотографии", '');
					if(anonym_email && IsValidEmail(anonym_email)) {
						$('#anonym_email').val(anonym_email);
					}
					else {
						alert('Вы должны ввести правильный email');
						return false;
					}
				}
			}

			$.getJSON('http://fotoifolder.ru/get_upload_params_crossdomain/?user_params=a2a7a442b3e93df8&jsoncallback=?', {
				type: 		"json",
				is_ajax:	'true',
				is_cover:	'true',
				rand:		Math.ceil(100000*Math.random())
			}, function(json) {
				$('#ifolder_foto_upload_form').attr('action', json.upload.upload_url+'&rand='+Math.ceil(100000*Math.random()));

				$('#ifolder_foto_upload_params').val(json.upload.upload_params);
				$('#ifolder_foto_clone').val(json.upload.clone);
				$('#ifolder_foto_upload_host').val(json.upload.upload_host);
				$('#ifolder_foto_return_path').val(json.upload.return_path);
				$('#ifolder_foto_progress_bar').val(json.upload.progress_bar);

				$('#ifolder_foto_upload_form').attr('upload_params_set', 'set')
				$('#ifolder_foto_upload_form').unbind('submit');
				$('#ifolder_foto_upload_form').submit();
			});

			return $('#ifolder_foto_upload_form').attr('upload_params_set') ? true : false;
		}

	</script>

	
